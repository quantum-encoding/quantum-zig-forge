  },
  "execution": {
    "max_threads": 4,
    "order_rate_limit": 200,
    "tick_buffer_size": 10000,
    "execution_mode": "paper"
  },
  "monitoring": {
    "report_interval": 60,
    "performance_metrics": true,
    "send_alerts": false
  },
  "status": "active",
