{
  "tenant_id": "GOLDEN_001",
  "name": "Midas Market Maker",
  "tier": "GOLD",
  "api_credentials": {
    "api_key": "${ALPACA_API_KEY}",
    "api_secret": "${ALPACA_API_SECRET}",
    "base_url": "https://paper-api.alpaca.markets"
  },
  "algorithm": {
    "type": "market_maker",
    "strategy": "hft_system",
    "parameters": {
      "max_position": 1000,
      "max_spread": 0.10,
